//Autor: Acosta Osorio Christian
#include <stdio.h>

int main() {
    int i;
    for (i = 1; i <= 100; i++) {
        if (i % 2 == 0) {
            printf("Larry");
        }
        if (i % 3 == 0) {
            printf("Moe");
        }
        if (i % 5 == 0) {
            printf("Curly");
        }
        if (i % 2 != 0 && i % 3 != 0 && i % 5 != 0) {
            printf("%d", i);
        }
        printf("\n");
    }
    return 0;
}
